{
  "version": 2,
  "builds": [
    {
      "src": "vercel-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.+\\.(css|js|png|jpg|jpeg|gif|svg|ico|webp))$",
      "dest": "/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "vercel-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "vercel-server.js"
    }
  ]
}